<div class="banner text-center">
  <div class="container">
    <h1>Create a Bookmark</h1>
  </div>
</div>
<div class="container w-50 my-4">
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit class:'btn btn-success' %>
  <% end %>
</div>
